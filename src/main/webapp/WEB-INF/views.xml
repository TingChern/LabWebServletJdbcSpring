<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="secure.login"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/secure/login.jsp"></property>
	</bean>
	<bean id="index"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index.jsp"></property>
	</bean>
	<bean id="product.fail"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property value="/pages/product.jsp" name="url"></property>
	</bean>
	<bean id="product.success"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property value="/pages/display.jsp" name="url"></property>
	</bean>
</beans>
